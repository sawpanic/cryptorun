{
  "dashboard": {
    "id": null,
    "title": "CryptoRun - Real-time Momentum Scanner",
    "tags": ["cryptorun", "momentum", "scanner", "trading"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "exchange",
          "type": "query",
          "query": "label_values(cryptorun_provider_healthy, venue)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": "*",
          "current": {
            "value": "$__all",
            "text": "All"
          }
        },
        {
          "name": "regime",
          "type": "custom",
          "options": [
            {
              "value": "0",
              "text": "Choppy"
            },
            {
              "value": "1", 
              "text": "Bull Trending"
            },
            {
              "value": "2",
              "text": "High Volatility"
            }
          ],
          "current": {
            "value": "1",
            "text": "Bull Trending"
          }
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Regime Changes",
          "datasource": "Prometheus",
          "expr": "increase(cryptorun_regime_switches_total[5m]) > 0",
          "iconColor": "rgba(255, 96, 96, 1)",
          "enable": true
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{job=\"cryptorun\"}",
            "legendFormat": "Service Status",
            "refId": "A"
          },
          {
            "expr": "process_uptime_seconds / 3600",
            "legendFormat": "Uptime (hours)",
            "refId": "B"
          },
          {
            "expr": "cryptorun_active_scans",
            "legendFormat": "Active Scans",
            "refId": "C"
          },
          {
            "expr": "sum(cryptorun_provider_healthy)",
            "legendFormat": "Healthy Providers",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.8},
                {"color": "green", "value": 1}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "title": "Provider Health Status",
        "type": "stat",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "cryptorun_provider_healthy{venue=~\"$exchange\"}",
            "legendFormat": "{{venue}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "DOWN"},
              {"type": "value", "value": "1", "text": "UP"}
            ]
          }
        }
      },
      {
        "id": 3,
        "title": "Current Market Regime",
        "type": "stat",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "cryptorun_active_regime",
            "legendFormat": "Active Regime",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "blue", "value": 0},
                {"color": "green", "value": 1},
                {"color": "orange", "value": 2}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "Choppy"},
              {"type": "value", "value": "1", "text": "Bull Trending"},
              {"type": "value", "value": "2", "text": "High Vol"}
            ]
          }
        }
      },
      {
        "id": 4,
        "title": "Provider Success Rates",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
        "targets": [
          {
            "expr": "cryptorun_provider_success_rate{venue=~\"$exchange\"}",
            "legendFormat": "{{venue}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {"type": "linear"},
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {"group": "A", "mode": "none"},
              "thresholdsStyle": {"mode": "off"}
            },
            "min": 0,
            "max": 1,
            "unit": "percentunit",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0.8},
                {"color": "yellow", "value": 0.9},
                {"color": "green", "value": 0.95}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Response Times (P95)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(cryptorun_step_duration_seconds_bucket[5m]))",
            "legendFormat": "{{step}} P95",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {"type": "linear"},
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {"group": "A", "mode": "none"},
              "thresholdsStyle": {"mode": "line"}
            },
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.3},
                {"color": "red", "value": 1.0}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Pipeline Throughput",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 22},
        "targets": [
          {
            "expr": "rate(cryptorun_pipeline_steps_total{status=\"success\"}[5m])",
            "legendFormat": "{{step}} Success/sec",
            "refId": "A"
          },
          {
            "expr": "rate(cryptorun_pipeline_steps_total{status=\"error\"}[5m])",
            "legendFormat": "{{step}} Error/sec",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {"type": "linear"},
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {"group": "A", "mode": "none"},
              "thresholdsStyle": {"mode": "off"}
            },
            "unit": "reqps"
          }
        }
      },
      {
        "id": 7,
        "title": "Cache Performance",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 30},
        "targets": [
          {
            "expr": "cryptorun_cache_hit_ratio",
            "legendFormat": "Hit Ratio",
            "refId": "A"
          },
          {
            "expr": "rate(cryptorun_cache_hits_total[5m])",
            "legendFormat": "{{cache_type}} Hits/sec",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {"type": "linear"},
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {"group": "A", "mode": "none"},
              "thresholdsStyle": {"mode": "line"}
            },
            "min": 0,
            "max": 1,
            "unit": "percentunit",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0.7},
                {"color": "yellow", "value": 0.85},
                {"color": "green", "value": 0.9}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Circuit Breaker Status",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 30},
        "targets": [
          {
            "expr": "cryptorun_circuit_breaker_state",
            "legendFormat": "{{name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 2}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "CLOSED"},
              {"type": "value", "value": "1", "text": "HALF-OPEN"},
              {"type": "value", "value": "2", "text": "OPEN"}
            ]
          }
        }
      },
      {
        "id": 9,
        "title": "Memory Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 38},
        "targets": [
          {
            "expr": "go_memstats_alloc_bytes / 1024 / 1024",
            "legendFormat": "Allocated MB",
            "refId": "A"
          },
          {
            "expr": "go_memstats_sys_bytes / 1024 / 1024",
            "legendFormat": "System MB",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {"type": "linear"},
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {"group": "A", "mode": "none"},
              "thresholdsStyle": {"mode": "off"}
            },
            "unit": "decmbytes"
          }
        }
      },
      {
        "id": 10,
        "title": "WebSocket Latencies",
        "type": "timeseries", 
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 38},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(cryptorun_ws_latency_ms_bucket{exchange=~\"$exchange\"}[5m]))",
            "legendFormat": "{{exchange}} P95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(cryptorun_ws_latency_ms_bucket{exchange=~\"$exchange\"}[5m]))",
            "legendFormat": "{{exchange}} P99",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {"type": "linear"},
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {"group": "A", "mode": "none"},
              "thresholdsStyle": {"mode": "line"}
            },
            "unit": "ms",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 300}
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Regime Health Indicators", 
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 46},
        "targets": [
          {
            "expr": "cryptorun_regime_health{indicator=\"volatility_7d\"}",
            "legendFormat": "7-Day Volatility",
            "refId": "A"
          },
          {
            "expr": "cryptorun_regime_health{indicator=\"above_ma_pct\"}",
            "legendFormat": "% Above 20MA",
            "refId": "B"
          },
          {
            "expr": "cryptorun_regime_health{indicator=\"breadth_thrust\"}",
            "legendFormat": "Breadth Thrust",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 20,
              "gradientMode": "none",
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {"type": "linear"},
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {"group": "A", "mode": "none"},
              "thresholdsStyle": {"mode": "off"}
            },
            "min": 0,
            "max": 1,
            "unit": "percentunit"
          }
        }
      },
      {
        "id": 12,
        "title": "Error Analysis",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 54},
        "targets": [
          {
            "expr": "sum by (step, error_type) (rate(cryptorun_pipeline_errors_total[5m]))",
            "legendFormat": "",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.01},
                {"color": "red", "value": 0.1}
              ]
            },
            "unit": "reqps"
          }
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "indexByName": {},
              "renameByName": {
                "step": "Pipeline Step",
                "error_type": "Error Type", 
                "Value": "Errors/sec"
              }
            }
          }
        ]
      }
    ]
  },
  "overwrite": true
}